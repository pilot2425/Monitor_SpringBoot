{
    "dashboard": {
      "id": null,
      "title": "Spring Boot Metrics",
      "panels": [
        {
          "title": "Total HTTP Requests",
          "type": "stat",
          "targets": [
            {
              "expr": "http_requests_total",
              "format": "time_series"
            }
          ]
        },
        {
          "title": "Request Latency",
          "type": "graph",
          "targets": [
            {
              "expr": "http_request_latency",
              "format": "time_series"
            }
          ]
        },
        {
          "title": "Log Lines Count",
          "type": "stat",
          "targets": [
            {
              "expr": "log_lines_total",
              "format": "time_series"
            }
          ]
        }
      ]
    }
  }
  